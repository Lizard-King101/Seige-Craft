{
  "name": "Seige-Craft",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "main": "main.js",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "electron": "electron .",
    "electron-build": "ionic build && electron .",
    "watch": "ionic build",
    "start": "ionic build && electron-forge start",
    "package": "ionic build && electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@ionic-native/core": "4.7.0",
    "@ionic-native/splash-screen": "4.7.0",
    "@ionic-native/status-bar": "4.7.0",
    "@ionic/storage": "2.1.3",
    "angularfire2": "^5.0.0-rc.10",
    "discord-rpc": "^3.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^5.0.4",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "ng-socket-io": "^0.2.4",
    "noisejs": "^2.1.0",
    "npm-watch": "^0.4.0",
    "rxjs": "5.5.11",
    "simple-element-resize-detector": "^1.2.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@azerion/phaser-spine": "^3.0.9",
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-deb": "^6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron-forge/maker-zip": "^6.0.0-beta.45",
    "@ionic/app-scripts": "^3.2.4",
    "electron": "^18.3.7",
    "expose-loader": "^0.7.5",
    "phaser": "^3.20.1",
    "typescript": "^2.9.2",
    "webpack-merge": "^4.1.4"
  },
  "description": "An Ionic project",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icon/seige-game-icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Seige_Craft"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
